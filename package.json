{
  "name": "tower-fetch",
  "version": "1.0.4",
  "description": "Easy fetch wrapper",
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist/*.js",
    "dist/*.cjs",
    "dist/src/*.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vite-plugin-dts": "^1.4.0"
  },
  "author": {
    "name": "SX3",
    "email": "i4s@ro.ru"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/svayex/tower-fetch.git"
  },
  "keywords": [
    "tower",
    "fetch",
    "browser",
    "client",
    "request",
    "api",
    "get",
    "ajax",
    "axios",
    "alternative"
  ]
}
